{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
      "foreground-light": "#e0e0e0",
      "foreground-dark": "#082336",
      "background-path": "#f30cf3",
      "background-git": "#ffff13",
      "background-dotnet": "#6CA35E",
      "background-root": "#ff9838",
      "background-time": "#385cfd",
      "background-exit": "#2e9599"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "template": " \ue5ff {{ path .Path .Location }} ",
          "properties": {
            "style": "powerlevel"
          },
          "background": "p:background-path",
          "foreground": "p:foreground-light",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "leading_diamond": "\ue0b6"
        },
        {
          "type": "git",
          "template": " {{ url .UpstreamIcon .UpstreamURL }}{{ url .HEAD .Env.OhMyPoshCustomBranchUri }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "ignore_status": [
              "C:/s/0/src",
              "C:/s/1/src",
              "C:/s/2/src"
            ]
          },
          "background": "p:background-git",
          "foreground": "p:foreground-dark",
          "style": "diamond",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "dotnet",
          "template": " \ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": true
          },
          "background": "p:background-dotnet",
          "foreground": "p:foreground-light",
          "style": "diamond",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "executiontime",
          "template": " \u23f1\ufe0f {{ .FormattedMs }} ",
          "properties": {
            "style": "austin",
            "threshold": 1000
          },
          "background": "p:background-time",
          "foreground": "p:foreground-light",
          "style": "diamond",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "exit",
          "template": " {{ if gt .Code 0 }}üëé{{ else }}üëç{{ end }} ",
          "properties": {
            "always_enabled": false
          },
          "background": "p:background-exit",
          "foreground": "p:foreground-light",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "style": "diamond",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "root",
          "background": "p:background-root",
          "foreground": "p:foreground-dark",
          "style": "diamond",
          "trailing_diamond": "\ue0b4"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
